<div class="posts">
    <% posts.forEach(post => { %>
      <div class="post">
        <a href="<%= post.link %>"><%= post.title %></a>
        <p>Published: <%= post.pubDate %></p>
        <p>Creator: <%= post.creator %></p>
        <p>Category: <%= post.category %></p>
        <p><%= post.content %></p>
      </div>
    <% }); %>
  </div>
  